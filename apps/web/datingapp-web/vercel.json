{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/datingapp-web/package.json",
      "use": "@vercel/vite"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "yarn workspaces run build && yarn workspace datingapp-web build",
  "installCommand": "yarn install --frozen-lockfile",
  "outputDirectory": "dist",
  "framework": "vite"
}
